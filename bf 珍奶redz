-- [3. 動態模式 (保留數字)]
local Patterns = {
    ["Level"] = "等級", ["Beli"] = "貝里", ["Fragments"] = "碎片",
    ["Health"] = "血量", ["Energy"] = "體力", ["Ping"] = "延遲",
    ["FPS"] = "幀數", ["Bounty"] = "賞金", ["Mastery"] = "精通",
    ["Distance"] = "距離", ["Price"] = "價格", ["Stocks"] = "庫存",
    ["Required"] = "需要", ["Damage"] = "傷害", ["Time"] = "時間",
    ["Skill Z"] = "技能 Z", ["Skill X"] = "技能 X",
    ["Skill C"] = "技能 C", ["Skill V"] = "技能 V", ["Skill F"] = "技能 F",
    ["Transformation"] = "變身", ["Cooldown"] = "冷卻"
}

-- [4. 強制掃描引擎 (Force Scanner)]
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")

local function TranslateObject(object)
    if not object then return end
    
    -- 檢查 Text, PlaceholderText, ToolTip
    local props = {"Text", "PlaceholderText", "ToolTip"}
    for _, prop in pairs(props) do
        if pcall(function() return object[prop] end) and object[prop] and object[prop] ~= "" then
            local originalText = object[prop]
            local newText = originalText
            local changed = false
            
            -- 1. 精確匹配 (優先)
            if UIDictionary[originalText] then
                object[prop] = UIDictionary[originalText]
                return
            end
            
            -- 2. 正則模式匹配
            for eng, chn in pairs(Patterns) do
                if string.find(newText, eng) then
                    newText = string.gsub(newText, eng, chn)
                    changed = true
                end
            end
            
            -- 3. 補漏匹配 (僅長詞)
            if not changed then
                for eng, chn in pairs(UIDictionary) do
                    if #eng >= 4 and string.find(newText, eng) then
                        newText = string.gsub(newText, eng, chn)
                        changed = true
                    end
                end
            end
            
            if changed then object[prop] = newText end
        end
    end
end

-- [5. 雙重執行循環 (Dual Loop)]
task.spawn(function()
    task.wait(4)
    game.StarterGui:SetCore("SendNotification", {Title="SYSTEM: APEX", Text="終極融合漢化已啟動", Duration=5})
    
    while true do
        task.wait(1) -- 1秒掃描一次，避免卡頓
        pcall(function()
            -- 掃描 CoreGui (外掛介面)
            for _, gui in pairs(CoreGui:GetChildren()) do
                if gui:IsA("ScreenGui") and gui.Name ~= "RobloxGui" and gui.Name ~= "TopBar" and gui.Name ~= "CoreScripts" then
                    for _, v in pairs(gui:GetDescendants()) do
                        TranslateObject(v)
                    end
                end
            end
            
            -- 掃描 PlayerGui (備用方案，防止外掛藏在這裡)
            if Players.LocalPlayer and Players.LocalPlayer:FindFirstChild("PlayerGui") then
                for _, gui in pairs(Players.LocalPlayer.PlayerGui:GetChildren()) do
                    if gui:IsA("ScreenGui") then
                        for _, v in pairs(gui:GetDescendants()) do
                            TranslateObject(v)
                        end
                    end
                end
            end
        end)
    end
end)
