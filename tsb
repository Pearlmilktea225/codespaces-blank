-- [[ üßã ÁèçÂ•∂ËÖ≥Êú¨ | TSB ÊâãÊ©üÊ•µÈôê Mega Pack Áâà ]] --
-- [[ Ë£Ω‰ΩúËÄÖ BY ÁèçÂ•∂ ]] --

do
    if not game:IsLoaded() then game.Loaded:Wait() end
    
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local UserInputService = game:GetService("UserInputService")
    local Workspace = game:GetService("Workspace")
    local CoreGui = game:GetService("CoreGui")
    local Lighting = game:GetService("Lighting")
    local TeleportService = game:GetService("TeleportService")
    local LocalPlayer = Players.LocalPlayer
    local Camera = Workspace.CurrentCamera
    
    -- Ê∏ÖÁêÜËàä‰ªãÈù¢
    for _, g in pairs(CoreGui:GetChildren()) do
        if g.Name == "Jenna_Mega_UI" then g:Destroy() end
    end

    -- Ê†∏ÂøÉÂèÉÊï∏Ë®≠ÂÆöÂ∫´
    getgenv().Config = {
        Target = "",
        IsFlinging = false, AntiFling = false, AntiVoid = false, NoClip = false,
        CustomSpeed = 16, CustomJump = 50, AutoTrash = false, AntiUlt = false,
        SavedPosition = nil,
        -- Êñ∞Â¢ûÂäüËÉΩÂèÉÊï∏
        Camlock = false, HitboxExpand = false, HitboxSize = 10,
        AirWalk = false, AirWalkPart = nil, SpinBot = false,
        FullBright = false, PotatoMode = false, ESP = false,
        AutoRejoin = false
    }

    -- ==========================================
    -- üé® Â∞àÊ•≠Áâà GUI ‰ªãÈù¢Ë®≠Ë®à (ÊîØÊè¥Ë∂ÖÈï∑ÊªæÂãï)
    -- ==========================================
    local ScreenGui = Instance.new("ScreenGui", CoreGui)
    ScreenGui.Name = "Jenna_Mega_UI"
    ScreenGui.IgnoreGuiInset = true

    local MainFrame = Instance.new("Frame", ScreenGui)
    MainFrame.Size = UDim2.new(0, 420, 0, 550)
    MainFrame.Position = UDim2.new(0.5, -210, 0.5, -275)
    MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 18)
    MainFrame.Active = true
    MainFrame.Draggable = true
    Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 12)
    Instance.new("UIStroke", MainFrame).Color = Color3.fromRGB(255, 180, 200)
    Instance.new("UIStroke", MainFrame).Thickness = 1.5

    local Header = Instance.new("Frame", MainFrame)
    Header.Size = UDim2.new(1, 0, 0, 45)
    Header.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 12)
    local HeaderBlocker = Instance.new("Frame", Header)
    HeaderBlocker.Size = UDim2.new(1, 0, 0, 10)
    HeaderBlocker.Position = UDim2.new(0, 0, 1, -10)
    HeaderBlocker.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    HeaderBlocker.BorderSizePixel = 0

    local Title = Instance.new("TextLabel", Header)
    Title.Size = UDim2.new(1, 0, 1, 0)
    Title.BackgroundTransparency = 1
    Title.Text = "üßã ÁèçÂ•∂ËÖ≥Êú¨ | Ê•µÈôê MEGA Áâà"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 18

    local Container = Instance.new("ScrollingFrame", MainFrame)
    Container.Size = UDim2.new(1, -20, 1, -55)
    Container.Position = UDim2.new(0, 10, 0, 50)
    Container.BackgroundTransparency = 1
    Container.ScrollBarThickness = 3
    local UIList = Instance.new("UIListLayout", Container)
    UIList.Padding = UDim.new(0, 8)
    Container.AutomaticCanvasSize = Enum.AutomaticSize.Y

    -- // UI ÁîüÊàêÂ∑•ÂÖ∑ //
    local function CreateTitle(text)
        local Label = Instance.new("TextLabel", Container)
        Label.Text = "‚îÅ‚îÅ " .. text .. " ‚îÅ‚îÅ"
        Label.Size = UDim2.new(1, 0, 0, 30)
        Label.BackgroundTransparency = 1
        Label.TextColor3 = Color3.fromRGB(255, 180, 200)
        Label.Font = Enum.Font.GothamBlack
        Label.TextXAlignment = Enum.TextXAlignment.Center
        Label.TextSize = 15
    end

    local function CreateToggle(text, callback)
        local Frame = Instance.new("Frame", Container)
        Frame.Size = UDim2.new(1, 0, 0, 45)
        Frame.BackgroundColor3 = Color3.fromRGB(22, 22, 28)
        Instance.new("UICorner", Frame).CornerRadius = UDim.new(0, 8)
        
        local Label = Instance.new("TextLabel", Frame)
        Label.Text = "  " .. text
        Label.Size = UDim2.new(0.75, 0, 1, 0)
        Label.BackgroundTransparency = 1
        Label.TextColor3 = Color3.new(1, 1, 1)
        Label.Font = Enum.Font.GothamSemibold
        Label.TextXAlignment = Enum.TextXAlignment.Left
        Label.TextSize = 14
        
        local Button = Instance.new("TextButton", Frame)
        Button.Size = UDim2.new(0, 70, 0, 30)
        Button.Position = UDim2.new(1, -80, 0.5, -15)
        Button.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
        Button.Text = "ÈóúÈñâ"
        Button.TextColor3 = Color3.fromRGB(255, 80, 80)
        Button.Font = Enum.Font.GothamBold
        Instance.new("UICorner", Button).CornerRadius = UDim.new(0, 6)
        
        local Toggled = false
        Button.MouseButton1Click:Connect(function()
            Toggled = not Toggled
            Button.Text = Toggled and "ÈñãÂïü" or "ÈóúÈñâ"
            Button.BackgroundColor3 = Toggled and Color3.fromRGB(80, 180, 100) or Color3.fromRGB(40, 40, 45)
            Button.TextColor3 = Toggled and Color3.new(1,1,1) or Color3.fromRGB(255, 80, 80)
            callback(Toggled)
        end)
    end

    local function CreateInput(placeholder, callback)
        local Box = Instance.new("TextBox", Container)
        Box.Size = UDim2.new(1, 0, 0, 40)
        Box.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
        Box.PlaceholderText = placeholder
        Box.Text = ""
        Box.TextColor3 = Color3.fromRGB(255, 255, 255)
        Box.Font = Enum.Font.Gotham
        Box.TextSize = 13
        Instance.new("UICorner", Box).CornerRadius = UDim.new(0, 8)
        Box.FocusLost:Connect(function() callback(Box.Text) end)
    end

    local function CreateButton(text, callback)
        local Btn = Instance.new("TextButton", Container)
        Btn.Size = UDim2.new(1, 0, 0, 40)
        Btn.BackgroundColor3 = Color3.fromRGB(50, 40, 60)
        Btn.Text = text
        Btn.TextColor3 = Color3.new(1, 1, 1)
        Btn.Font = Enum.Font.GothamBold
        Btn.TextSize = 14
        Instance.new("UICorner", Btn).CornerRadius = UDim.new(0, 8)
        Btn.MouseButton1Click:Connect(callback)
    end

    -- ==========================================
    -- üöÄ ÂäüËÉΩÂçÄÂ°äÂª∫Êßã
    -- ==========================================

    CreateTitle("üéØ Êà∞È¨•ËàáËøΩÊìäÁ≥ªÁµ±")
    CreateInput("Ëº∏ÂÖ•ÁõÆÊ®ôÂêçÁ®± (‰æã: Player123)", function(val) getgenv().Config.Target = val end)
    
    local FlingConn
    CreateToggle("ÁµÇÊ•µÈªèËëóÁî©È£õ (Ê≠ª‰∫°Ëá™ÂãïÁ∫åÈÄ£)", function(state)
        getgenv().Config.IsFlinging = state
        if state then
            local Char = LocalPlayer.Character
            if Char and Char:FindFirstChild("HumanoidRootPart") then getgenv().Config.SavedPosition = Char.HumanoidRootPart.CFrame end
            
            FlingConn = RunService.Heartbeat:Connect(function()
                if not getgenv().Config.IsFlinging then return end
                local C = LocalPlayer.Character
                local TargetPlr
                for _, v in pairs(Players:GetPlayers()) do
                    if v ~= LocalPlayer and string.sub(v.Name:lower(), 1, #getgenv().Config.Target) == getgenv().Config.Target:lower() then 
                        TargetPlr = v break 
                    end
                end

                if C and C:FindFirstChild("HumanoidRootPart") and TargetPlr and TargetPlr.Character and TargetPlr.Character:FindFirstChild("HumanoidRootPart") then
                    local TRP = TargetPlr.Character.HumanoidRootPart
                    local LRP = C.HumanoidRootPart
                    LRP.CFrame = TRP.CFrame
                    LRP.AssemblyLinearVelocity = Vector3.zero
                    LRP.AssemblyAngularVelocity = Vector3.new(50000, 50000, 50000)
                    for _, p in pairs(C:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide = false p.Massless = true end end
                end
            end)
        else
            if FlingConn then FlingConn:Disconnect() end
            local C = LocalPlayer.Character
            if C and C:FindFirstChild("HumanoidRootPart") then
                C.HumanoidRootPart.Velocity = Vector3.zero
                C.HumanoidRootPart.AssemblyAngularVelocity = Vector3.zero
                if getgenv().Config.SavedPosition then task.wait(0.1) C.HumanoidRootPart.CFrame = getgenv().Config.SavedPosition end
            end
        end
    end)

    CreateToggle("ÈéñÂÆöÁõÆÊ®ôË¶ñËßí (Camlock)", function(state) getgenv().Config.Camlock = state end)
    
    CreateInput("Ë®≠ÂÆöÂà§ÂÆöÁÆ±Â§ßÂ∞è (È†êË®≠10, ÊúÄÂ§ß30)", function(val) 
        local size = tonumber(val)
        if size then getgenv().Config.HitboxSize = math.clamp(size, 5, 30) end
    end)
    CreateToggle("Êì¥Â§ßÊïµ‰∫∫Á¢∞ÊíûÁÆ± (Hitbox Expander)", function(state) getgenv().Config.HitboxExpand = state end)

    CreateTitle("üõ°Ô∏è Èò≤Ë≠∑ËàáÂèçÊÆ∫Á≥ªÁµ±")
    CreateToggle("Èò≤Ê≠ªÊã≥/Â§ßÊãõ (Ë°ÄÈáèÈ©üÈôçÊãñÂÖ•ËôõÁ©∫ÊÆ∫)", function(state) getgenv().Config.AntiUlt = state end)
    CreateToggle("ÁµïÂ∞çÈò≤ËôõÁ©∫ (Èò≤ÊéâËêΩÂà§ÂÆö)", function(state) getgenv().Config.AntiVoid = state end)
    CreateToggle("Èò≤ÂΩàÈ£õ (Èò≤Ê≠¢Ë¢´Áï∞Â∏∏ÊìäÈ£õ)", function(state) getgenv().Config.AntiFling = state end)

    CreateTitle("üèÉ Áâ©ÁêÜËàáÁßªÂãï‰øÆÊîπ")
    CreateInput("Ëá™Ë®ÇÁßªÂãïÈÄüÂ∫¶ (Êé®Ëñ¶: 30-50)", function(val) getgenv().Config.CustomSpeed = tonumber(val) or 16 end)
    CreateInput("Ëá™Ë®ÇË∑≥Ë∫çÈ´òÂ∫¶ (Êé®Ëñ¶: 80-150)", function(val) getgenv().Config.CustomJump = tonumber(val) or 50 end)
    
    CreateToggle("Á©øÁâÜÊ®°Âºè (NoClip)", function(state) getgenv().Config.NoClip = state end)
    CreateToggle("Ë∏èÁ©∫Ë°åËµ∞ (Air Walk - ËÖ≥‰∏ãÁîüÊàêÈö±ÂΩ¢Êùø)", function(state)
        getgenv().Config.AirWalk = state
        if state and not getgenv().Config.AirWalkPart then
            local part = Instance.new("Part", Workspace)
            part.Size = Vector3.new(5, 1, 5)
            part.Anchored = true
            part.Transparency = 1
            getgenv().Config.AirWalkPart = part
        elseif not state and getgenv().Config.AirWalkPart then
            getgenv().Config.AirWalkPart:Destroy()
            getgenv().Config.AirWalkPart = nil
        end
    end)
    CreateToggle("ËÆäË∫´ÈôÄËû∫ (SpinBot - Ê∑∑Ê∑ÜÊïµ‰∫∫Ë¶ñÁ∑ö)", function(state) getgenv().Config.SpinBot = state end)

    CreateTitle("üëÅÔ∏è Ë¶ñË¶∫ËàáÊïàËÉΩÂ∑•ÂÖ∑")
    CreateToggle("È¶¨Èà¥ËñØÊ®°Âºè (Ê•µËá¥Èôç‰ΩéÁï´Ë≥™/FPSÈò≤Âç°)", function(state)
        getgenv().Config.PotatoMode = state
        if state then
            Lighting.GlobalShadows = false
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") then v.Material = Enum.Material.SmoothPlastic end
                if v:IsA("Decal") or v:IsA("Texture") then v.Transparency = 1 end
            end
        end
    end)
    CreateToggle("Â§úË¶ñÊ®°Âºè (FullBright)", function(state)
        getgenv().Config.FullBright = state
        Lighting.Ambient = state and Color3.new(1,1,1) or Color3.fromRGB(138, 138, 138)
        Lighting.Brightness = state and 2 or 1
    end)
    CreateToggle("Áé©ÂÆ∂ÈÄèË¶ñ (ESP È°ØÁ§∫Ë°ÄÈáè/Ë∑ùÈõ¢)", function(state) getgenv().Config.ESP = state end)

    CreateTitle("üõ†Ô∏è Ëá™ÂãïÂåñËàáÂØ¶Áî®Â∑•ÂÖ∑")
    CreateToggle("ÂÖ®Ëá™ÂãïÊíøÂûÉÂúæ (ÂÖçÈªûÊìä)", function(state)
        getgenv().Config.AutoTrash = state
        if state then
            task.spawn(function()
                while getgenv().Config.AutoTrash do
                    task.wait(2)
                    local Char = LocalPlayer.Character
                    if Char and Char:FindFirstChild("HumanoidRootPart") then
                        for _, obj in pairs(Workspace:GetDescendants()) do
                            if obj:IsA("ProximityPrompt") and string.find(obj.Parent.Name:lower(), "trash") then
                                local origPos = Char.HumanoidRootPart.CFrame
                                Char.HumanoidRootPart.CFrame = obj.Parent.CFrame
                                task.wait(0.2) fireproximityprompt(obj) task.wait(0.1)
                                Char.HumanoidRootPart.CFrame = origPos
                                break
                            end
                        end
                    end
                end
            end)
        end
    end)
    
    CreateButton("Êèõ‰º∫ÊúçÂô® (Server Hop)", function()
        game:GetService("TeleportService"):Teleport(game.PlaceId, LocalPlayer)
    end)
    
    CreateButton("‰∏ÄÈçµËá™ÊÆ∫ (ÈáçÁΩÆÁãÄÊÖã)", function()
        local Char = LocalPlayer.Character
        if Char and Char:FindFirstChild("Humanoid") then Char.Humanoid.Health = 0 end
    end)

    -- ==========================================
    -- üß† Ê†∏ÂøÉÂ§ßËø¥Âúà (ÊïàËÉΩÂÑ™ÂåñÁâàÔºåÊï¥ÂêàÊâÄÊúâÈÅãÁÆó)
    -- ==========================================
    
    RunService.RenderStepped:Connect(function()
        local Char = LocalPlayer.Character
        if not Char or not Char:FindFirstChild("HumanoidRootPart") then return end
        local HRP = Char.HumanoidRootPart
        
        -- 1. ÈÄüÂ∫¶‰øÆÊîπ (CFrame Êé®Êì†)
        if getgenv().Config.CustomSpeed > 16 and Char:FindFirstChild("Humanoid") then
            local MoveDir = Char.Humanoid.MoveDirection
            if MoveDir.Magnitude > 0 then
                HRP.CFrame = HRP.CFrame + (MoveDir * (getgenv().Config.CustomSpeed / 100))
            end
        end

        -- 2. Camlock Ë¶ñËßíÈéñÂÆö
        if getgenv().Config.Camlock and getgenv().Config.Target ~= "" then
            for _, v in pairs(Players:GetPlayers()) do
                if v ~= LocalPlayer and string.sub(v.Name:lower(), 1, #getgenv().Config.Target) == getgenv().Config.Target:lower() then
                    if v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                        Camera.CFrame = CFrame.new(Camera.CFrame.Position, v.Character.HumanoidRootPart.Position)
                    end
                    break
                end
            end
        end

        -- 3. Ë∏èÁ©∫Ë°åËµ∞ (AirWalk)
        if getgenv().Config.AirWalk and getgenv().Config.AirWalkPart then
            getgenv().Config.AirWalkPart.CFrame = HRP.CFrame * CFrame.new(0, -3.5, 0)
        end

        -- 4. SpinBot
        if getgenv().Config.SpinBot then
            HRP.CFrame = HRP.CFrame * CFrame.Angles(0, math.rad(50), 0)
        end
    end)

    RunService.Stepped:Connect(function()
        local Char = LocalPlayer.Character
        if not Char or not Char:FindFirstChild("HumanoidRootPart") then return end
        local HRP = Char.HumanoidRootPart

        -- Èò≤ËôõÁ©∫
        if getgenv().Config.AntiVoid and HRP.Position.Y < -20 and HRP.Position.Y > -400 then
            HRP.Velocity = Vector3.zero
            HRP.CFrame = CFrame.new(HRP.Position.X, 100, HRP.Position.Z)
        end

        -- Á©øÁâÜ
        if getgenv().Config.NoClip then
            for _, part in pairs(Char:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide then part.CanCollide = false end
            end
        end

        -- Èò≤ÂΩàÈ£õ
        if getgenv().Config.AntiFling and not getgenv().Config.IsFlinging then
            if HRP.Velocity.Magnitude > 200 then
                HRP.Velocity = Vector3.zero
                HRP.AssemblyAngularVelocity = Vector3.zero
            end
        end

        -- Hitbox Expander (Êì¥Â§ßÁ¢∞ÊíûÁÆ±)
        if getgenv().Config.HitboxExpand then
            for _, v in pairs(Players:GetPlayers()) do
                if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    local targetHRP = v.Character.HumanoidRootPart
                    targetHRP.Size = Vector3.new(getgenv().Config.HitboxSize, getgenv().Config.HitboxSize, getgenv().Config.HitboxSize)
                    targetHRP.Transparency = 0.5
                    targetHRP.BrickColor = BrickColor.new("Bright red")
                    targetHRP.CanCollide = false
                end
            end
        else
            -- Âæ©ÂéüÁ¢∞ÊíûÁÆ±
            for _, v in pairs(Players:GetPlayers()) do
                if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    local targetHRP = v.Character.HumanoidRootPart
                    if targetHRP.Size.X > 5 then
                        targetHRP.Size = Vector3.new(2, 2, 1)
                        targetHRP.Transparency = 1
                    end
                end
            end
        end
    end)

    -- // Ë∑≥Ë∫çÁõ£ËÅΩ //
    local isJumping = false
    UserInputService.JumpRequest:Connect(function()
        if getgenv().Config.CustomJump > 50 and not isJumping then
            isJumping = true
            local Char = LocalPlayer.Character
            if Char and Char:FindFirstChild("HumanoidRootPart") then
                Char.HumanoidRootPart.Velocity = Vector3.new(Char.HumanoidRootPart.Velocity.X, getgenv().Config.CustomJump, Char.HumanoidRootPart.Velocity.Z)
            end
            task.wait(0.2)
            isJumping = false
        end
    end)

    -- // Ê≠ªÊã≥/ÈáçÁîüËá™ÂãïÁ∫åÈÄ£ËàáÁõ£Êéß //
    LocalPlayer.CharacterAdded:Connect(function(newChar)
        if getgenv().Config.IsFlinging then
            task.wait(1.5)
            game.StarterGui:SetCore("SendNotification", {Title = "Âæ©Ê¥ª", Text = "ÁπºÁ∫åËøΩÊìäÁõÆÊ®ô...", Duration = 2})
        end
        local Hum = newChar:WaitForChild("Humanoid")
        Hum.HealthChanged:Connect(function(newHealth)
            if getgenv().Config.AntiUlt then
                if Hum.MaxHealth - newHealth > 40 or newHealth < 15 then
                    local RP = newChar:FindFirstChild("HumanoidRootPart")
                    if RP then RP.CFrame = CFrame.new(0, -500, 0) RP.Velocity = Vector3.zero end
                end
            end
        end)
    end)

    -- ESP Ê®ôÁ±§Á≥ªÁµ±
    local function UpdateESP()
        for _, v in pairs(Players:GetPlayers()) do
            if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("Head") then
                local bg = v.Character.Head:FindFirstChild("JennaESP")
                if getgenv().Config.ESP then
                    if not bg then
                        bg = Instance.new("BillboardGui", v.Character.Head)
                        bg.Name = "JennaESP"
                        bg.Size = UDim2.new(0, 100, 0, 40)
                        bg.StudsOffset = Vector3.new(0, 2, 0)
                        bg.AlwaysOnTop = true
                        local txt = Instance.new("TextLabel", bg)
                        txt.Size = UDim2.new(1,0,1,0)
                        txt.BackgroundTransparency = 1
                        txt.TextColor3 = Color3.fromRGB(255, 100, 100)
                        txt.Font = Enum.Font.GothamBold
                        txt.TextSize = 12
                    end
                    local hum = v.Character:FindFirstChild("Humanoid")
                    if hum then
                        local dist = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude)
                        bg.TextLabel.Text = v.Name .. "\n[" .. math.floor(hum.Health) .. " HP] " .. dist .. "m"
                    end
                else
                    if bg then bg:Destroy() end
                end
            end
        end
    end
    task.spawn(function() while true do task.wait(0.5) UpdateESP() end end)

    -- ==========================================
    -- ü™ü Êá∏ÊµÆÁ∏ÆÊîæÊåâÈàï (ÊãñÊõ≥ÂúìÂΩ¢ Icon)
    -- ==========================================
    local ToggleBtn = Instance.new("ImageButton", ScreenGui)
    ToggleBtn.Size = UDim2.new(0, 50, 0, 50)
    ToggleBtn.Position = UDim2.new(0.05, 0, 0.05, 0)
    ToggleBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    ToggleBtn.Image = "rbxassetid://13465637257"
    Instance.new("UICorner", ToggleBtn).CornerRadius = UDim.new(1, 0)
    Instance.new("UIStroke", ToggleBtn).Color = Color3.fromRGB(255, 180, 200)
    Instance.new("UIStroke", ToggleBtn).Thickness = 2
    
    local Open = true
    ToggleBtn.MouseButton1Click:Connect(function()
        Open = not Open
        MainFrame.Visible = Open
    end)

    local dragToggle, dragInput, dragStart, startPos
    ToggleBtn.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragToggle = true dragStart = input.Position startPos = ToggleBtn.Position
        end
    end)
    ToggleBtn.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragToggle then
            local delta = input.Position - dragStart
            ToggleBtn.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragToggle = false end
    end)

end
